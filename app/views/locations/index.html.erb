<div class="row">
  <div class="col l12 m12 s12">
    <br/>
    <% if logged_in? && current_user.admin? %>
        <span>New Location &nbsp </span>
     <%= link_to new_workplace_location_path(@workplace.id), class: 'location-button btn-floating btn-small waves-effect waves-light blue' do %>
        <i class="location-icon material-icons">+</i>
    <% end %>
    <% end %>
    <h3><% if @workplace.workplace_name != 'Not Listed' %>
          Locations for <%= @workplace.workplace_name %> </h3>
          <% end %>
    <%= render 'shared/location_feed' %>
  </div>
</div>