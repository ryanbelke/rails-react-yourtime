<% if request.original_url.to_s.include? 'workplace' %>
  <div class="col l4 m4 s12">
    <div class="paper workplaces card">
      <div class="card-image">
        <%= image_tag "siliconlabs.png" %>
        <span class="card-title"><%= link_to workplace.workplace_name, workplace_categories_path(workplace.slug) %></span>
      </div>
      <div class="card-content">
        <h5>Address: </h5> <span><%= workplace.workplace_address %><hr /></span>
        <hr /><br/>
        <p><%= workplace.workplace_description %></p>
        <br />
        <div>
         <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3446.0269689298643!2d-97.75064508406118!3d30.264812781802878!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89e3707f9b4075a9%3A0xdd6cbdcffe4e1785!2sSilicon+Labs!5e0!3m2!1sen!2sus!4v1511041859622" width="100%" height="150" frameborder="0" style="border:0" allowfullscreen></iframe>
        </div>
      </div>
      <div class="card-action">
        <%= link_to "View property info", workplace_path(workplace.id) %>
        <li class="no-border" id="workplace-<%= workplace.id %>">
          <% if logged_in? && current_user.admin? %>
        <span class="timestamp">
          Created <%= time_ago_in_words(workplace.created_at) %> ago.
          </span>
            <%= link_to "delete", workplace, method: :delete,
                        data: { confirm: "You sure?" } %>
            &nbsp; <%= link_to "edit", edit_workplace_path(workplace.id) %>
          <% end %>
        </li>
      </div>
    </div>
  </div>

    <% else %>
    <li id="workplace-<%= workplace.id %>">
      <span class="user"><%= link_to workplace.workplace_name, workplace_path(workplace.slug) %></span>
      <% if logged_in? && current_user.admin? %>
          <br /><span>create new category</span>
          <%= link_to new_workplace_category_path(workplace) do |f| %>
              <span class="location-icon material-icons">+</span>

          <% end %>
      <span class="timestamp">
        <%= link_to "delete", workplace, method: :delete,
                    data: { confirm: "You sure?" } %>
    &nbsp;  <%= link_to "edit", edit_workplace_path(workplace.id) %>
      <% end %>
      </span>
    </li>
<% end %>
