<% if request.original_url.to_s.include? 'sections' %>
  <ul class="flex-row">
    <% if @section_feed_items.any? %>
      <%= render @section_feed_items %>
    <% end %>
  </ul>
<% else %>
  <ul class="feed-container">
    <% if @section_feed_items.any? %>
      <%= render @section_feed_items %>
    <% end %>
  </ul>
<% end %>


