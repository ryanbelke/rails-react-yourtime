
<tbody>
<tr>
  <td><%= booking.booking_status %></td>
  <td><%= Date.parse(booking.schedule.date.to_s).strftime("%m/%d/%Y") %></td>
  <td>
    <% booking.service_id.each do |service| %>
      <%= get_service_name(service) %>
      <br />
    <% end %>
  </td>
  <td><%= get_location_name(booking.location_id) %></td>
  <td><%= number_to_currency(booking.booking_price) %></td>
  <td id="table-buttons">
    <br />
    <%= link_to 'View', user_booking_path(current_user.id, booking.id),
  class: 'waves-effect waves-light btn custom-btn-small2 blue lighten-1' %></td>
  <td id="table-buttons">
    <br/>
    <% if booking.booking_status != 'Complete' %>
      <%= link_to 'Edit', user_booking_path(booking.user_id, booking.id),
                class: 'waves-effect waves-light btn custom-btn-small2 blue lighten-1' %>
    <% end %>
  </td>

</tr>
</tbody>